{
  "name": "@codabiat-monorepo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev:web": "pnpm --filter @codabiat-monorepo/web-client dev",
    "dev:api": "pnpm --filter @codabiat-monorepo/api dev",
    "dev:admin": "pnpm --filter @codabiat-monorepo/admin-dashboard dev",
    "build:web": "pnpm --filter @codabiat-monorepo/web-client build",
    "build:api": "pnpm --filter @codabiat-monorepo/api build",
    "build:admin": "pnpm --filter @codabiat-monorepo/admin-dashboard build",
    "build:packages": "pnpm --filter @codabiat/types build && pnpm --filter @codabiat/database build && pnpm --filter @codabiat/auth build",
    "build": "pnpm run build:packages && pnpm run build:web && pnpm run build:api && pnpm run build:admin",
    "start": "pnpm run dev:web",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules apps/*/dist packages/*/dist .pnpm-store pnpm-lock.yaml"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@next/eslint-plugin-next": "^16.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^4.0.0",
    "eslint": "^9.8.0",
    "eslint-config-next": "^16.0.1",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.0.0",
    "vitest": "^4.0.0"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@types/d3": "^7.4.3",
    "@types/three": "^0.182.0",
    "bcryptjs": "^3.0.3",
    "d3": "^7.9.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "mongoose": "^9.0.2",
    "next": "~16.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "6.29.0",
    "three": "^0.182.0"
  }
}
